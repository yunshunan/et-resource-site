# API 配置
VUE_APP_API_BASE_URL=https://api.example.com
VUE_APP_API_TIMEOUT=30000

# 隐私配置
VUE_APP_ENCRYPTION_KEY=${ENCRYPTION_KEY}
VUE_APP_PRIVACY_POLICY_VERSION=1.0
VUE_APP_DATA_RETENTION_DAYS=90
VUE_APP_MAX_EXPORT_SIZE=1GB

# 安全配置
VUE_APP_SESSION_TIMEOUT=1800
VUE_APP_MAX_LOGIN_ATTEMPTS=3
VUE_APP_PASSWORD_MIN_LENGTH=12

# 第三方服务配置
VUE_APP_GOOGLE_ANALYTICS_ID=${GOOGLE_ANALYTICS_ID}
VUE_APP_SENTRY_DSN=${SENTRY_DSN}

# 性能监控配置
VUE_APP_PERFORMANCE_SAMPLE_RATE=0.05
VUE_APP_ERROR_SAMPLE_RATE=1.0
VUE_APP_SLOW_REQUEST_THRESHOLD=5000

# 数据收集配置
VUE_APP_COLLECT_ANALYTICS=true
VUE_APP_COLLECT_ERROR_LOGS=true
VUE_APP_COLLECT_PERFORMANCE=true
VUE_APP_COLLECT_USER_BEHAVIOR=true

# 数据共享配置
VUE_APP_SHARE_ANALYTICS=false
VUE_APP_SHARE_ERROR_LOGS=false
VUE_APP_SHARE_PERFORMANCE=false

# 通知配置
VUE_APP_NOTIFY_POLICY_UPDATES=true
VUE_APP_NOTIFY_DATA_BREACHES=true
VUE_APP_NOTIFY_EXPORT_COMPLETE=true

# 导出配置
VUE_APP_EXPORT_FORMAT=json
VUE_APP_EXPORT_ENCRYPTION=true
VUE_APP_EXPORT_COMPRESSION=true

# 日志配置
VUE_APP_LOG_LEVEL=error
VUE_APP_LOG_RETENTION_DAYS=30
VUE_APP_LOG_MAX_SIZE=100MB

# 缓存配置
VUE_APP_CACHE_ENABLED=true
VUE_APP_CACHE_MAX_AGE=7200
VUE_APP_CACHE_MAX_SIZE=200MB

# 调试配置
VUE_APP_DEBUG_MODE=false
VUE_APP_VERBOSE_LOGGING=false
VUE_APP_SHOW_ERROR_DETAILS=false 